{
  "presets": ["react-native"],
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "alias": {
          "store": "./src/store",
          "actions": "./src/store/actions",
          "reducers": "./src/store/reducers",
          "screens": "./src/screens",
          "assets": "./src/assets",
          "configs": "./src/configs",
          "services": "./src/services",
          "components": "./src/components"
        }
      }
    ]
  ]
}
